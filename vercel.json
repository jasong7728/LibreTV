{
  "rewrites": [
    {
      "source": "/proxy/:path*",
      "destination": "/api/proxy/:path*"
    },
    {
      "source": "/s=:query",
      "destination": "/index.html"
    },
    {
      "source": "/player.html",
      "destination": "/index.html"
    },
    {
      "source": "/player.html/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/about.html",
      "destination": "/index.html"
    },
    {
      "source": "/watch.html",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}
